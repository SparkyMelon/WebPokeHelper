<template id="pokemonImgTemplate">
    <span class="textInTable"><img class="pokemonImg" src="~/All Pokemon Images/[ID].png" /></span>
</template>

<template id="pokemonTypeTemplate">
    <span class="textInTable"><img class="pokemonTypeImg" src="~/Pokemon Icons/[TYPE].png" /></span>
</template>

<template id="pokemonAbilityTemplate">
    <div class="row rows2">
        <div class="col-3 pokemonDataColFix border borderTeamColour_xy borderFix">
            <span class="textInTable">[ABILITY NAME]</span>
        </div>
        <div class="col-9 pokemonDataColFix border borderTeamColour_xy borderFix">
            <span class="textInTable"><em>[ABILITY DESC]</em></span>
        </div>
    </div>
</template>

