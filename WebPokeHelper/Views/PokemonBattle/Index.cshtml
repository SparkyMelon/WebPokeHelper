@{
    ViewData["Title"] = "Pokemon Battle";
}

@section Scripts
{
    <script type="text/javascript" src=@Url.Content("~/js/BodyScrollLock.js")></script>
    <script type="text/javascript" src=@Url.Content("~/js/PokemonBattleScript.js")></script>
    <link rel="stylesheet" type="text/css" href=@Url.Content("~/css/PokemonBattleStyle.css")>
    <script type="text/javascript">
        var model = @Html.Raw(Newtonsoft.Json.JsonConvert.SerializeObject(Model));
    </script>

    <script type="text/javascript" src=@Url.Content("~/js/Require.js")></script>
}

<div class="container pokemonBattleContainer">
