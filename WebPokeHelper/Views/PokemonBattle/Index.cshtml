                                <div class="backgroundTeamColour_@(x)@(y)">
                                    @for (int j = 0; j < Model.Count; j++)
                                    {
                                        <a class="dropdown-item dropdownItem_@(x)@(y)">@Model[j].name</a>
                                    }
                                </div>
                            </div>
                        </div>
                        <div class="container " id="pokemon_@(x)@(y)" style="background-image: url()">
